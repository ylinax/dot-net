{
  "help": "https://go.microsoft.com/fwlink/?linkid=866610",
  "root": true,

  "dependentFileProviders": {
    "add": {
      "pathSegment": {
        "add": {
          "ExtensionMethods.*": [ ".cs" ],
          "Models\\*.*": [ "*.cs" ],
          "Current.*": [ "*.cs" ]
        }
      },
      "extensionToExtension": {
        "add": {
          ".css": [ ".less" ],
          ".js": [ ".ts", ".tsx" ],
          ".js.map": [ ".js" ],
        }
      }
    }
  }
}